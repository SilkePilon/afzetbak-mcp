{
  "name": "@afzetbak/mcp-server",
  "module": "src/index.ts",
  "type": "module",
  "version": "1.0.2",
  "description": "MCP Server for afzetbak.nl - Real-time container/dumpster information and availability service",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "afzetbak.nl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/afzetbak/mcp-server.git"
  },
  "keywords": [
    "mcp",
    "afzetbak",
    "container",
    "dumpster",
    "rental",
    "real-time",
    "netherlands"
  ],
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "tsc",
    "build:http": "tsc && cp dist/index.js dist/http.js",
    "dev": "bun --watch src/index.ts",
    "start:http": "bun run src/server/http-server.ts",
    "dev:http": "bun --watch src/server/http-server.ts",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.1"
  },
  "peerDependencies": {
    "@valibot/to-json-schema": "^1.0.0",
    "effect": "^3.14.4",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.12",
    "cors": "^2.8.5",
    "fastmcp": "^1.21.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.24.2"
  }
}
